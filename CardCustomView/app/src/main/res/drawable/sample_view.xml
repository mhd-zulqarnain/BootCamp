<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:maxLevel="0">
        <rotate
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:fromDegrees="65"
            android:pivotX="135%"
            android:pivotY="14%"
